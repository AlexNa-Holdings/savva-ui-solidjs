}
.sv-docs a:hover { opacity: 0.9; }

.sv-docs blockquote {
  margin: 1rem 0;
  padding: 0.6rem 0.9rem;
  border-left: 3px solid hsl(var(--border));
  background: hsl(var(--accent));
